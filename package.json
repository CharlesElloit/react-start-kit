    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.5.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "style-loader": "^1.3.0",
