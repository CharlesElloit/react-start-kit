{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-eval": "error",
    "no-debugger": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "react/prop-types": 0,
    "react/forbid-prop-types": 0
  }
}
